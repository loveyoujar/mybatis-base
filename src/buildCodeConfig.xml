<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<properties>
	<!-- 基础包名称 -->
	<entry key="basepackage">com.ltq.demo</entry>

	<!--模块名称 -->
	<entry key="modelName">User</entry>

	<!--项目名称 -->
	<entry key="projectName">demo</entry>

	<!-- 类作者 -->
	<entry key="author">admin</entry>

	<!-- 类描述 -->
	<entry key="describe">用户信息</entry>

	<!-- 创建时间 -->
	<entry key="createtime">2019年9月22日</entry>

	<!-- 类名称 -->
	<entry key="className">User</entry>



	<!-- 表名称<config_table中的key> -->
	<entry key="tableName">user</entry>

	<!-- 表主键列名称 -->
	<entry key="pkName">id</entry>

	<!-- 数据库名称 -->
	<entry key="dataBaseName">user</entry>

