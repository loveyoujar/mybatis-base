	<!-- 数据库URL -->
	<entry key="jdbc_url">jdbc:mysql://127.0.0.1:3306/user?useUnicode=true&amp;characterEncoding=UTF-8</entry>
	
	<!-- 用户名 -->
	<entry key="jdbc_username">root</entry>

	<!-- 密码 -->
	<entry key="jdbc_password">123456</entry>

	<!-- 输出根目录 -->
	<entry key="outRoot">./generator-output</entry>
